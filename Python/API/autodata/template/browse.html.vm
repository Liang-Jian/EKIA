<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html
        PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
        "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
    <title>案例详情</title>
    <style type="text/css">
        {
            text-align: left
        }
    </style>
</head>

<body>
<div align="left">
    <ul style="list-style: none">
        <li>
            <div align="center">
                <b>案例编号:</b><span id="casecode">${flowTestCase.casecode}</span>
            </div>
        </li>
        <br/>
        <li>
            <b>正反例:</b><span id="caseProperty">${flowTestCase.caseProperty}</span>
        </li>
        <li>
            <b>案例名称:</b><span id="caseTitle">${flowTestCase.caseTitle}</span>
        </li>
        <li>
            <b>操作步骤:</b><span id="executeStep">${flowTestCase.executeStep}</span>
        </li>
        <li>
            <b>预期结果:</b><span id="expectResult">${flowTestCase.expectResult}</span>
        </li>
        <li>
            <b>执行时间:</b><span id="finishTime">${flowTestCase.finishTime}</span>
        </li>
        <li>
            <b>执行状态:</b><span id="executeStatus">${flowTestCase.executeStatus}</span>
        </li>
        <br/>
        #foreach($caseEntity in ${flowTestCase.testCaseEntityList})
            <li>
                <b>$caseEntity.apiName</b><br/>
                <b>请求报文:</b><br/>
                <textarea rows="20" cols="220" id="reqString">${caseEntity.reqPlainString}</textarea>
            </li>
            <br/>
            <li>
                <b>$caseEntity.apiName</b><br/>
                <b>返回报文:</b><br/>
                <textarea rows="20" cols="220" id="respString">${caseEntity.responseString}</textarea>
            </li>
            <li>
                <b>$caseEntity.apiName</b><br/>
                <b>接口返回数据:</b>
                <table border="1" cellspacing="0">
                    <thead>
                    <tr>
                        <th bgcolor="aqua">数据名</th>
                        <th bgcolor="aqua">数据值</th>
                    </tr>
                    </thead>
                    <tbody>
                        #foreach($dataKey in ${caseEntity.respMapData.keySet()})
                        <tr>
                            <td>$dataKey</td>
                            <td>${caseEntity.respMapData.get($dataKey)}</td>
                        </tr>
                        #end
                    </tbody>
                </table>
            </li>

            <br/>
        #end
        <br/>
        <li>
            <b>流程数据:</b>
            <table border="1" cellspacing="0">
                <thead>
                <tr>
                    <th bgcolor="aqua">数据名</th>
                    <th bgcolor="aqua">数据值</th>
                </tr>
                </thead>
                <tbody>
                    #foreach($dataKey in ${flowTestCase.flowData.keySet()})
                    <tr>
                        <td>$dataKey</td>
                        <td>${flowTestCase.flowData.get($dataKey)}</td>
                    </tr>
                    #end
                ##                    <tr>
                ##                        <td>保单号</td>
                ##                        <td>${flowTestCase.flowData.policyNO}</td>
                ##                    </tr>
                </tbody>
            </table>
        </li>
        <br/>
        <li>
            <b>执行日志:</b><br/>
            <textarea rows="20" cols="220" id="caseLog">${flowTestCase.caseLog}</textarea>
        </li>
    </ul>
</div>
</body>
</html>